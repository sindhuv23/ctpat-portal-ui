    "font-awesome": "^4.7.0",
    "material-design-icons": "^3.0.1",
    "material-icons": "^1.12.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-cookie-service": "^14.0.0",
    "rxjs": "^6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
