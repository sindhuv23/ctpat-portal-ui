{
  "name": "ctpat-pui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "ng build --baseHref=/ctpat-pui/ --configuration production=false --configuration=dev --stats-json",
    "build:proxyDev": "node --max_old_space_size=64384  ./node_modules/@angular/cli/bin/ng build --baseHref=/nace/ace/ctpat/ctpat-pui/ --configuration=dev --build-optimizer --output-hashing=none",
    "build-prod": "ng build --baseHref=/ace/ctpat/ctpat-pui/ --configuration production=true",
    "build:proxyRel": "node --max_old_space_size=64384  ./node_modules/@angular/cli/bin/ng build --baseHref=/nace/ace/ctpat/ctpat-pui/ --configuration production --build-optimizer --output-hashing=none",
    "build-report": "ng build --configuration production=true --stats-json && npm run bundle-analysis",
    "bundle-analysis": "webpack-bundle-analyzer dist/ace-ctpat/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "gc":"ng g c svi-certification-email-details-modal"
  },
  "private": true,
